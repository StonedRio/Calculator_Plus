<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Adjust the corner radius to make the button more or less rounded -->
    <corners android:radius="8dp" />

    <!-- Define the background color of the button -->
    <solid android:color="#33384b" />

    <!-- Optionally, you can add a stroke around the button -->
    <stroke
        android:width="1dp"
        android:color="#33384b" />

</shape>